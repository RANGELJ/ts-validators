{
  "name": "ts-validators",
  "version": "9.0.5",
  "description": "A way to validate unknown values in typescript",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "deploy": "node ./scripts/deploy.mjs"
  },
  "exports": {
      "./*": {
        "import": "./es6/*.mjs",
        "require": "./cjs/*.js",
        "types": "./types/*.d.ts"
      }
  },
  "author": "Rangel Peralta Jorge Luis",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/node": "18.7.8",
    "execa": "6.1.0",
    "fs-extra": "10.1.0",
    "typescript": "4.7.4"
  }
}
